{
  "Ocelot": {
    "BaseUrl": "https://localhost:7048"
  },

  "ServiceUrls": {
    "Store": {
      "Endpoints": {
        "GetAllStore": "store/all?page={page}&pageSize={pageSize}",
        "GetStoreDetail": "store/public?storeId={storeId}"
      }
    },
    "Product": {
      "Endpoints": {
        "GetByStore": "buyerproduct/search/store?storeId={storeId}",
        "SearchCategories": "buyerproduct/search/categories?storeId={storeId}"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
